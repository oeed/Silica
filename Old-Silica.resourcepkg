local function tidy( path )\
    return path\
        :gsub( \"/.-/%.%./\", \"/\" )\
        :gsub( \"^.-/%.%./\", \"\" )\
        :gsub( \"/%./\", \"/\" )\
        :gsub( \"^%.%./\", \"\" )\
        :gsub( \"^%.%.$\", \"\" )\
        :gsub( \"//+\", \"/\" )\
        :gsub( \"^[^/]\", \"/\" )\
        :gsub( \"/$\", \"\" )\
end\
