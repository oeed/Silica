local function sortSpecial( a )\
    local flip;\
    local temp;\
    \
    repeat\
        flip = false\
        for i = 1, #a - 1 do\
            if ( a[i+1] >= 0 and a[i] > a[i+1] ) or ( a[i] < 0 and a[i+1] >= 0 ) then\
\9\9\9\9flip = true\
\9\9\9\9temp = a[i]\
\9\9\9\9a[i] = a[i+1]\
\9\9\9\9a[i+1] = temp\
\9\9\9end\
\9\9end\
