
class FileAssociation {

	name = String;
	mime = String;
	icon = Icon;
	extensions = Table;
	programBundle = Bundle.allowsNil;
	argumentFormat = String( "%1" );
	
}

function FileAssociation:initialise( String name, String mime, Icon icon, Table extensions, String argumentFormat, Bundle programBundle )
	self.name = name
	self.mime = mime
	self.icon = icon
	self.extensions = extensions
	self.programBundle = programBundle
	self.argumentFormat = argumentFormat
end
