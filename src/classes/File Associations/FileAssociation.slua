
class FileAssociation {

	mime = String;
	icon = Icon;
	extensions = Table;
	programPath = String;
	argumentFormat = String( "%1" );
	
}

function FileAssociation:initialise( String mime, Icon icon, Table extensions, String programPath, String argumentFormat )
	self.mime = mime
	self.icon = icon
	self.extensions = extensions
	self.programPath = programPath
	self.argumentFormat = argumentFormat
end
