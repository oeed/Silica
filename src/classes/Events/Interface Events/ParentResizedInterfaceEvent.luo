class ParentResizedInterfaceEvent extends InterfaceEvent 
	
	static default eventType = "interface_parent_resized";
	
	property Boolean isHorizontal = false
	property Boolean isVertical = false
	default isSentToSender = false
	default isSentToChildren = true

	function initialise( Boolean isHorizontal, Boolean isVertical, Container sender )
		self.isHorizontal = isHorizontal
		self.isVertical = isVertical
		self.sender = sender
	end


end