class ChildRemovedInterfaceEvent extends InterfaceEvent 

	static default eventType = "interface_child_removed";
	
	property Container container
	property View childView -- the added child
	default isSentToChildren = false
	default isSentToParents = true

	--[[
		@constructor
		@desc Creates a child removed event from the arguments
		@param [View] childView -- the removed child
	]]
	function initialise( View childView, Container container )
		self.childView = childView
		self.container = container
	end

end