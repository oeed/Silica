class ParentChangedInterfaceEvent extends InterfaceEvent 
	
	static default eventType = "interface_parent_changed";
	
	property Container newParent -- the new parent
	property Container oldParent -- the old parent
	default isSentToChildren = false

	function initialise( Container newParent, Container oldParent )
		self.newParent = newParent
		self.oldParent = oldParent
	end

end