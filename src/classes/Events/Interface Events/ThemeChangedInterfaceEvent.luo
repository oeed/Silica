class ThemeChangedInterfaceEvent extends InterfaceEvent 

	static default eventType = "interface_theme_changed";
	
	property Theme newTheme
	property Theme oldTheme

	--[[
		@constructor
		@desc Creates a theme changed event from the arguments
		@param newTheme -- the theme that is now active
		@param oldTheme -- the theme that was previously active
	]]
	function initialise( Theme newTheme, Theme oldTheme )
		self.newTheme = newTheme
		self.oldTheme = oldTheme
	end


end