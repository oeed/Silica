
class HomeSymbol extends Symbol 

	
	static property String symbolName = "home"
	static property Number width = 9
	static property Number height = 9
	


	static function initialise()
		local path = Path( self.width, self.height, 5, 1 )
		path:lineTo( 9, 5 )
		path:lineTo( 8, 5 )
		path:lineTo( 8, 9 )
		path:lineTo( 6, 9 )
		path:lineTo( 6, 6 )
		path:lineTo( 4, 6 )
		path:lineTo( 4, 9 )
		path:lineTo( 2, 9 )
		path:lineTo( 2, 5 )
		path:lineTo( 1, 5 )
		path:close()

		super( path )
	end
end