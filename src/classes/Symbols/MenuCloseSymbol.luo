
class MenuCloseSymbol extends Symbol 

	static = {
		property String symbolName = "menuClose"
		property Number height = 4
	};


	static function initialise()
		local path = Path( self.width, self.height, 1, 4 )
		path:lineTo( 4, 1 )
		path:lineTo( 7, 4 )
		path:close()

		self:super( path )
	end

end